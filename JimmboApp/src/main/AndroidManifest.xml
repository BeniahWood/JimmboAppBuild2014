<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="at.infound.jimmboappbuild1.jimmboapp" >

    <activity android:name="at.infound.jimmboappbuild1.jimmboapp.LaunchScreen" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <activity
        android:name="at.infound.jimmboappbuild1.jimmboapp.MainScreen"
        android:label="@string/app_name" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="at.infound.jimmboappbuild1.jimmboapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="at.infound.jimmboappbuild1.jimmboapp.SplashActivity"
            android:label="@string/title_activity_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="at.infound.jimmboappbuild1.jimmboapp.NoAccount"
            android:label="@string/title_activity_no_account" >
        </activity>
        <activity
            android:name="at.infound.jimmboappbuild1.jimmboapp.NoPassword"
            android:label="@string/title_activity_no_password" >
        </activity>
        <activity
            android:name="at.infound.jimmboappbuild1.jimmboapp.ServerResults"
            android:label="@string/title_activity_server_results" >
        </activity>
        <activity
            android:name="at.infound.jimmboappbuild1.jimmboapp.LoggingIn"
            android:label="@string/title_activity_logging_in" >
        </activity>
        <activity
            android:name="at.infound.jimmboappbuild1.jimmboapp.LoggingInResult"
            android:label="@string/title_activity_logging_in_result" >
        </activity>
    </application>

    <supports-screens
        android:anyDensity="false"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true" />

</manifest>
